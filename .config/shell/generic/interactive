# NOTE: This file is sourced in fish shell by `~/.config/shell/fish/interactive`.
# If you need to use anything other than simple commands, this won't work anymore.

# Setup terminal escape bindings.
stty intr ^X
stty quit ^Q
stty erase undef
stty kill ^K
stty eof ^E
stty start undef
stty stop undef
stty susp undef
stty rprnt undef
stty werase undef
stty lnext undef
stty discard undef

